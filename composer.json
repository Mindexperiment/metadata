{
    "name": "agpretto/metadata",
    "description": "Metadata package for Laravel applications",
    "keywords": [
        "metadata",
        "seo",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Mindexperiment/metadata",
    "support": {
        "issues": "https://github.com/Mindexperiment/metadata/issues",
        "source": "https://github.com/Mindexperiment/metadata"
    },
    "authors": [
        {
            "name": "Andrea Giuseppe Pretto",
            "email": "info@andreagiuseppe.com",
            "homepage": "https://andreagiuseppe.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "^5.0|^6.0",
        "illuminate/view": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Agpretto\\Metadata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Agpretto\\Metadata\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agpretto\\Metadata\\MetadataServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
